

@section TitleSection
{
    ATC Educational Institution || Test Schedule
}
@section StyleSection
{
    @*@Styles.Render("~/Content/css")*@
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Content/testschedule.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:200,300,400,500,600,700,800,900" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
}
@section MainSection
{
    @{
        Html.RenderPartial("~/Views/Common/_Menu.cshtml");
    }
    <div id="testgroup">
        <div class="container">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <h2>
                    TEST SCHEDULE
                </h2>
                @if (ViewBag.QuestionGroupList != null)
                {
                <table class="table">
                    <thead>
                        <tr>
                            <th>Test Name</th>
                            <th>Start</th>
                        </tr>
                    </thead>
                    <tbody>
                     @foreach (var x in ViewBag.QuestionGroupList)
                     {
                          <tr>
                                    <td class="test-name">
                                        @x.QuestionGroupname
                                    </td>
                                    <td class="start-link">
                                        <a href="/testdescription/@x.QuestionGroupId">START</a>
                                    </td>
                          </tr>
                     }
                    </tbody>
               </table>
                }
                else
                {
                    <div class="row errormessage">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="error">
                                !Error occured while fetching data from Data Base
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
    @{
        Html.RenderPartial("~/Views/Common/_Footer.cshtml");
    }
}

@section ScriptsSection
{
    <script src="~/scripts/jquery.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/owl.carousel.min.js"></script>
    <script src="~/scripts/custom.js"></script>
    <script src="~/scripts/testschedule.js"></script>
}